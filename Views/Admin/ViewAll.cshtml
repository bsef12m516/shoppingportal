@{
    ViewBag.Title = "ViewAll";
   
}
@{
    Layout = null;
}

@using OSP.Models;
<h1 style="font-family:'Segoe Script'"><b>Displaying All Items</b></h1>
<body>
    <div class="col-md-8">

            @foreach(var o in Model)
            {
                <br />
                <h3 style="font-family:'Segoe Print'"><b>Item Code:</b> @o.ItemCode</h3>
                <h3 style="font-family: 'Segoe Print'"><b>Price:</b> @o.Price</h3>
                <h3 style="font-family: 'Segoe Print'"><b>Description:</b>@o.Description</h3>
                <h3 style="font-family: 'Segoe Print'"><b>Category: </b>@o.Category</h3>
                <h3 style="font-family: 'Segoe Print'"><b>Stock: </b>@o.Stock</h3>
               
                
            }
            
      </div>
</body>
